<section
    id="magnifier"
    class="c-prompt"
    data-prompt
    aria-labelledby="title-magnifier"
    role="region">

    <div class="c-prompt__content">
        <h2
            id="title-magnifier"
            class="c-prompt__title">
            {{content.magnifier.title}}
        </h2>
        <p class="c-prompt__directions">
            {{{content.magnifier.directions}}} <a rel="external noopener" class="c-prompt__undo" href="">How to undo</a>
        </p>
        <p class="c-prompt__example">
            {{{content.magnifier.example}}}
        </p>
    </div>
</section>
